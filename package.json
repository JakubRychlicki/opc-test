{
  "name": "opc-new",
  "version": "1.0.0",
  "description": "Prosty projekt z bazÄ… danych",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:seed": "tsx src/database/seed.ts",
    "db:generate": "kysely-codegen --out-file ./src/db/types.ts --camel-case --runtime-enums --dialect mssql --env-file ./.envrc && prettier --write \"src/db/types.ts\"",
    "db:migrate-new": "kysely migrate:make --",
    "db:migrate-run": "kysely migrate:latest",
    "db:migrate-down": "kysely migrate:down --",
    "db:seed-new": "kysely seed:make --",
    "db:seed-run": "kysely seed:run",
    "import-data": "npx ts-node src/scripts/import-data.ts"
  },
  "keywords": [
    "database",
    "kysely"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "kysely-codegen": "^0.18.5",
    "kysely-ctl": "^0.14.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@tediousjs/connection-string": "^0.5.0",
    "kysely": "^0.28.2",
    "kysely-typeorm": "^0.3.0",
    "mssql": "^11.0.1",
    "node-opcua-client": "^2.156.0",
    "tarn": "^3.0.2",
    "tedious": "^18.6.1"
  }
}
